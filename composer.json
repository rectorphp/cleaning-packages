{
    "name": "rector/cleaning-packages",
    "description": "Meta composer-package to never forget any important dev packages in your project",
    "license": "MIT",
    "type": "metapackage",
    "bin": [
        "vendor/bin/jack",
        "vendor/bin/swiss-knife",
        "vendor/bin/parallel-lint",
        "vendor/bin/ecs",
        "vendor/bin/class-leak"
    ],
    "require": {
        "php": ">=7.2",
        "phpstan/extension-installer": ">=1.4",
        "phpstan/phpstan-doctrine": ">=2.0",
        "phpstan/phpstan-phpunit": ">=2.0",
        "phpstan/phpstan-symfony": ">=2.0",
        "phpecs/phpecs": ">=2.1",
        "symplify/phpstan-extensions": ">=12.0",
        "symplify/phpstan-rules": ">=14.6",
        "shipmonk/composer-dependency-analyser": ">=1.8",
        "rector/type-perfect": ">=2.1",
        "rector/swiss-knife": ">=2.2",
        "rector/jack": ">=0.2",
        "tomasvotruba/type-coverage": ">=2.0",
        "tomasvotruba/unused-public": ">=2.0",
        "tomasvotruba/class-leak": ">=2.0",
        "php-parallel-lint/php-parallel-lint": ">=1.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}

